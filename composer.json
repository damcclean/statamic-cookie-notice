{
    "name": "doublethreedigital/cookie-notice",
    "description": "Cookie Consent for Statamic - with support for consent groups!",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "DoubleThreeDigital\\CookieNotice\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DoubleThreeDigital\\CookieNotice\\Tests\\": "tests"
        }
    },
    "extra": {
        "download-dist": {
            "url": "https://github.com/duncanmcclean/cookie-notice/releases/download/{$version}/dist.tar.gz",
            "path": "resources/dist"
        },
        "statamic": {
            "name": "Cookie Notice",
            "description": "Cookie Consent for Statamic - with support for consent groups!"
        },
        "laravel": {
            "providers": [
                "DoubleThreeDigital\\CookieNotice\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "pixelfear/composer-dist-plugin": "^0.1.5",
        "statamic/cms": "3.3.*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "scripts": {
        "lint": [
            "php-cs-fixer fix ./src"
        ],
        "test": [
            "./vendor/bin/phpunit"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "minimum-stability": "beta"
}
